# ============================================================================
# Data Sources (APIs used by getPoolData and analyzePool)
# ============================================================================
# DeFiLlama - Pool data and APY information
DEFILLAMA_API_URL=https://yields.llama.fi

# Venus Protocol API - Venus markets data
VENUS_API_URL=https://api.venus.io

# CoinGecko - Token price data (CAKE, BNB)
COINGECKO_API_URL=https://api.coingecko.com/api/v3

# Owlracle - BSC gas price data
OWLRACLE_GAS_API=https://api.owlracle.info/v2/bsc/gas

# DexScreener - Real-time DEX pair data (V2/V3 volume and liquidity)
DEXSCREENER_API=https://api.dexscreener.com/latest/dex

# ============================================================================
# Gas & Transaction Settings
# ============================================================================
MAX_GAS_PRICE_GWEI=5
GAS_LIMIT_SWAP=300000
GAS_LIMIT_SUPPLY=200000
MAX_TX_VALUE_WEI=10000000000000000000000

# ============================================================================
# Agent Strategy
# ============================================================================
MIN_PROFIT_THRESHOLD_PERCENT=1
REALLOCATION_CHECK_INTERVAL_MS=300000
MIN_HOLDING_PERIOD_DAYS=7
PROFIT_RECOVERY_DAYS=7

# ============================================================================
# Notifications
# ============================================================================
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# ============================================================================
# Logging
# ============================================================================
LOG_LEVEL=info
LOG_FORMAT=json

# ============================================================================
# Dashboard (Next.js) - Client-Side Environment Variables
# ============================================================================
# REQUIRED: Get from https://cloud.walletconnect.com (free)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# REQUIRED: Get from https://dashboard.biconomy.io (free tier available)
# Biconomy MEE (Modular Execution Environment) - Single API Key for everything
# Create a MEE Stack project at: https://dashboard.biconomy.io
# This replaces the old bundler/paymaster URLs
NEXT_PUBLIC_BICONOMY_MEE_API_KEY=YOUR_MEE_API_KEY

# BNB Chain RPC (already configured)
NEXT_PUBLIC_BNB_RPC_URL=https://1rpc.io/bnb
